<?xml version="1.0" encoding="UTF-8" ?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Witrac\Domain\Spaceship\Model\Entity\Spaceship" table="spaceship">
        <id name="id" type="guid"/>

        <field name="name" length="100" />
        <field name="positionX" type="integer"/>
        <field name="positionY" type="integer"/>


        <unique-constraints>
            <unique-constraint name="U_name" columns="name"/>
        </unique-constraints>

        <indexes>
            <index name="IDX_name" columns="name"/>
        </indexes>
    </entity>
</doctrine-mapping>